{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "platforms": ["linux", "macOS", "windows"],
  "permissions": ["core:default",
    "core:window:default",
    "core:app:default",
    "core:event:default",
    "core:menu:default",
    "core:tray:default",
    "core:path:default",
    "core:resources:default",
    "core:image:default",
    "core:webview:default"]
}
